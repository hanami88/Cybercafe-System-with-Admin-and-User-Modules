    <body>
      <div class="container">
        <div class="nagavi">
          <h1>Admin</h1>
          <a href="/Home/Quanly">Quản lý máy</a>
          <a href="/Home/Quanlynguoidung">Quản lý tài khoản</a>
        </div>
        <h1 class="text">Thêm Máy Tính</h1>
        <div class="error">@ViewBag.error</div>
        <table>
          <thead>
            <tr>
              <th>Tên máy</th>
              <th>Trạng thái</th>
              <th>Đơn giá</th>
              <th>Thêm</th>
            </tr>
          </thead>
          <tbody>
            <form
              action="/Home/ThemMayTinh"
              method="post"
              style="display: inline"
            >
              <tr>
                <td><input type="text" name="Tenmay" id="" placeholder=""/></td>
                <td>Rảnh</td>
                <td><input type="text" name="dongia" id="" placeholder=""/></td>
                <td>
                  <button type="submit">Thêm</button>
                </td>
              </tr>
            </form>
          </tbody>
        </table>
      </div>
    </body>
